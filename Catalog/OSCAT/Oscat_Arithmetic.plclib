						<var name="B" type="REAL">
							<descr>b coefficient</descr>
						</var>
						<var name="C" type="REAL">
							<descr>c coefficient</descr>
						</var>
					</inputVars>
				</vars>
				<iecDeclaration active="FALSE"/>
				<sourceCode type="ST">
					<![CDATA[


(*
version 1.1	18. Mar. 2011
programmer 	hugo
